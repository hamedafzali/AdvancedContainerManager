# Application Configuration
NODE_ENV=production
PORT=5003
HOST=0.0.0.0
DEBUG_MODE=false
LOG_LEVEL=info

# Docker Configuration
DOCKER_HOST=unix:///var/run/docker.sock
DOCKER_TIMEOUT=2000
DOCKER_PROTOCOL=http

# Redis Configuration (Optional)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
